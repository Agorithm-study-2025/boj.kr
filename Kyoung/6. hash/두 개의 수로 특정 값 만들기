#include <iostream>
#include <vector>
#include <unordered_map>
using namespace std;
// vector<int> arr, int target
int main()
{
    int n; 
    cin>>n;
    int target;
    unordered_map<int, bool>m;
    int result = 0; // false
    vector<int>arr;
    for(int i=0; i<n; i++){
        int a;
        cin>>a;
        arr.push_back(a);
        m[arr[i]] = true;
    } // 1, 2, 3, 4, 8
    
    cin>>target;
    
    for(int i=0; i<n; i++){
        // 같은 숫자 더하는 것 방지지 & 존재하는지 확인
        if(arr[i] != target - arr[i] && m[target - arr[i]]){
            result = 1;
            break;
        }
        result = 0;
    }
    
    
    cout<<result;
    return 0;
}